    BLOCKED+=($ip)
done
for ip in "${NAN[@]}"
do
    BLOCKED+=($ip)
done
for ip in "${SA[@]}"
do
    BLOCKED+=($ip)
done
for ip in "${EUW[@]}"
do
    BLOCKED+=($ip)
done
for ip in "${EUN[@]}"
do
    BLOCKED+=($ip)
done
for ip in "${EUE[@]}"
do
    BLOCKED+=($ip)
done
for ip in "${POL[@]}"
do
    BLOCKED+=($ip)
done
for ip in "${SPA[@]}"
do
    BLOCKED+=($ip)
done
for ip in "${AS[@]}"
do
    BLOCKED+=($ip)
done
for ip in "${JP[@]}"
do
    BLOCKED+=($ip)
done
for ip in "${HK[@]}"
do
    BLOCKED+=($ip)
done
for ip in "${ME[@]}"
do
    BLOCKED+=($ip)
done
for ip in "${NAW[@]}"
do
    BLOCKED+=($ip)
done
for ip in "${INW[@]}"
do
    BLOCKED+=($ip)
done
for ip in "${INE[@]}"
do
    BLOCKED+=($ip)
done
for ip in "${AUS[@]}"
do
    BLOCKED+=($ip)
done
for ip in "${BLOCKED[@]}"
do
    sudo iptables -A INPUT -s $ip -j DROP
    echo "Blocking ip: "$ip
done
fi

