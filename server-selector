do
    BLOCKED+=($ip)
done
for ip in "${AF[@]}"
do
    BLOCKED+=($ip)
done
for ip in "${INW[@]}"
do
    BLOCKED+=($ip)
done
for ip in "${INE[@]}"
do
    BLOCKED+=($ip)
done
for ip in "${AUS[@]}"
do
    BLOCKED+=($ip)
done
for ip in "${BLOCKED[@]}"
do
    sudo iptables -A INPUT -s $ip -j DROP
    echo "Blocking ip: "$ip
done

fi

