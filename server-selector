# RESET
if [ $choice = "RESET" ]
then
# Unblock
for ip in "${NAE[@]}"
do
    BLOCKED+=($ip)
done
for ip in "${NAN[@]}"
do
    BLOCKED+=($ip)
done
for ip in "${SA[@]}"
do
    BLOCKED+=($ip)
done
for ip in "${EUW[@]}"
do
    BLOCKED+=($ip)
done
for ip in "${EUN[@]}"
do
    BLOCKED+=($ip)
