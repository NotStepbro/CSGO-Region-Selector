    BLOCKED+=($ip)
done
for ip in "${INE[@]}"
do
    BLOCKED+=($ip)
done
for ip in "${NAW[@]}"
do
    BLOCKED+=($ip)
done
for ip in "{AUS[@]}"
do
    Blocked+=($ip)
done

for ip in "${BLOCKED[@]}"
do
    sudo iptables -D INPUT -s $ip -j DROP
    echo "Unblocking ip: "$ip
done
fi
